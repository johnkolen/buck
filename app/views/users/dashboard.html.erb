<div class=row>
  <div class="col-sm-8 col-sm-offset-2 col-md-4 col-md-offset-4">
    <h1><%=@user.first_name%>'s Dashboard</h1>
    <% if flash[:notice] %>
      <div class=row>
	<h3><%= flash[:notice] %></h3>
      </div>
    <% end %>
    <div class=row>
      <%= render :partial=>"transfer" %>
    </div>
    <div class=row>
      <%= render :partial=>"transfer_list"%>
    </div>
    <div class=row>
      <%= logout_button %>
    </div>
  </div>
</div>
